{
  "name": "botbuilder-mcs",
  "version": "1.0.1",
  "description": "A sample bot that demonstrates some APIs of Microsoft's Cognitive Services.",
  "main": "lib/app.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/sebsylvester/botbuilder-mcs.git"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "pretest": "tsc test/*.ts --module commonjs --sourcemap",
    "pretest:watch": "tsc test/*.ts --module commonjs --sourcemap --watch",
    "test": "nyc --reporter=lcov mocha",
    "posttest": "nyc report --reporter=json && codecov -f coverage/*.json",
    "start": "node lib/app.js",
    "postinstall": "npm run build"
  },
  "keywords": [
    "Microsoft Bot Framework",
    "BotBuilder SDK",
    "Microsoft Cognitive Services",
    "Computer Vision API",
    "Emotion API",
    "chatbots",
    "bots"
  ],
  "author": "Sebastian Sylvester",
  "license": "MIT",
  "bugs": "https://github.com/sebsylvester/botbuilder-mcs/issues",
  "homepage": "https://github.com/sebsylvester/botbuilder-mcs",
  "dependencies": {
    "botbuilder": "^3.7.0",
    "lodash.sortby": "^4.7.0",
    "needle": "^1.5.0",
    "request": "^2.79.0",
    "restify": "^4.3.0",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "@types/chai": "^3.4.35",
    "@types/lodash.sortby": "^4.7.0",
    "@types/mocha": "^2.2.39",
    "@types/needle": "^1.4.0",
    "@types/node": "^7.0.5",
    "@types/request": "0.0.40",
    "@types/restify": "^2.0.41",
    "@types/sinon": "^1.16.35",
    "@types/valid-url": "^1.0.2",
    "chai": "^3.5.0",
    "codecov": "^1.0.1",
    "mocha": "^3.2.0",
    "nyc": "^10.1.2",
    "sinon": "^1.17.6",
    "typescript": "^2.1.6"
  }
}
